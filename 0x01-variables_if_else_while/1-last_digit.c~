#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - show The last digit of n
 *
 * Return: cero
 */
int main(void)
{
	int n;
	/*n is create random and show it*/
	srand(time(0));
	n = rand() - RAND_MAX / 2;
	n = (n % 10);
	if (n > 5)
		printf("is %d and is greater than 5\n", n);
	else if (0 < n < 6)
		printf("is %d and is less than 6 and not 0\n", n);
	else
		printf("is %d and is 0\n", n);
	return (0);
}
